{
  "name": "ssr-react-router-node-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "babel ./app -d build",
    "build:watch": "babel ./app -d build --watch",
    "build:client": "webpack --config ./webpack.config.js/",
    "build:watch:client": "webpack --config ./webpack.config.js/ --watch",
    "build:prod": "npm run build && npm run build:client",
    "start": "npm run build:prod && NODE_ENV=production node ./build/server/index.js",
    "start:dev": "concurrently \"npm run build:watch\" \"npm run build:watch:client\" \"nodemon ./build/server/index.js\"",
    "test": "jest --watch --coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.1.0",
    "html-webpack-plugin": "^2.30.1",
    "nodemon": "^1.11.0",
    "redux-devtools-extension": "^2.13.7",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  }
}
